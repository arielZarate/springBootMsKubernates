						{
							"name": "save inventory",
							"event": [
								{
									"listen": "prerequest",
									"script": {
										"exec": [
											"// ===== SCRIPT MEJORADO CON PERSISTENCIA =====\r",
											"\r",
											"// Configuración inicial\r",
											"const config = {\r",
											"    initialValue: 1000,\r",
											"    variableName: \"secuentialId\",\r",
											"    step: 1\r",
											"};\r",
											"\r",
											"// Función para generar ID secuencial\r",
											"function generateSequentialId() {\r",
											"    try {\r",
											"        // Obtener valor actual o usar valor inicial\r",
											"        let currentValue = parseInt(pm.environment.get(config.variableName));\r",
											"        \r",
											"        if (isNaN(currentValue)) {\r",
											"            currentValue = config.initialValue;\r",
											"            console.log(\"Inicializando contador con valor:\", currentValue);\r",
											"        }\r",
											"        \r",
											"        // Incrementar\r",
											"        const newValue = currentValue + config.step;\r",
											"        \r",
											"        // Guardar en entorno\r",
											"        pm.environment.set(config.variableName, newValue);\r",
											"                \r",
											"        console.log(\"ID generado:\", newValue);\r",
											"        return newValue;\r",
											"        \r",
											"    } catch (error) {\r",
											"        console.error(\"Error generando ID:\", error);\r",
											"        return config.initialValue;\r",
											"    }\r",
											"}\r",
											"\r",
											"// Ejecutar la función\r",
											"generateSequentialId();"
										],
										"type": "text/javascript",
										"packages": {}
									}
								}
